{
    "name": "pulunomoe/puluauth",
    "description": "PuluAuth is a partial OAuth2 server implementation in PHP.",
    "license": "MIT",
    "version": "0.1.0",
    "scripts": {
        "dev": "php -S localhost:8000 -t public public/index.php",
        "test": "phpunit --stop-on-defect"
    },
    "require": {
        "ext-gmp": "*",
        "ext-pdo": "*",
        "defuse/php-encryption": "^2.4",
        "league/oauth2-server": "^8.4",
        "monolog/monolog": "^3.7",
        "nyholm/psr7": "^1.8",
        "nyholm/psr7-server": "^1.1",
        "php-di/php-di": "^7.0",
        "psr/http-server-middleware": "^1.0",
        "ramsey/uuid": "^4.7",
        "slim/slim": "^4.14",
        "vlucas/phpdotenv": "^5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
